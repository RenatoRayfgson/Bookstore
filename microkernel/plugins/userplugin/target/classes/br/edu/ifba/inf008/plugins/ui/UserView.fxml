<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifba.inf008.plugins.ui.UserViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="txtName" layoutX="27.0" layoutY="92.0" prefHeight="31.0" prefWidth="173.0" />
            <TextField fx:id="txtEmail" layoutX="27.0" layoutY="168.0" prefHeight="31.0" prefWidth="173.0" />
            <Button fx:id="CreateButton" layoutX="18.0" layoutY="220.0" mnemonicParsing="false" onAction="#CreateButtonAction" text="Create" />
            <Button fx:id="UpdateButton" layoutX="80.0" layoutY="220.0" mnemonicParsing="false" onAction="#UpdateButtonAction" text="Update" />
            <Button fx:id="DeleteButton" layoutX="145.0" layoutY="220.0" mnemonicParsing="false" onAction="#DeleteButtonAction" text="Delete" />
            <TableView fx:id="UsersTable" layoutX="219.0" layoutY="36.0" prefHeight="325.0" prefWidth="389.0">
              <columns>
                <TableColumn fx:id="IdColumn" prefWidth="45.0" text="ID" />
                <TableColumn fx:id="NameColumn" prefWidth="110.0" text="Name" />
                  <TableColumn fx:id="EmailColumn" prefWidth="180.0" text="Email" />
              </columns>
            </TableView>
            <Label layoutX="27.0" layoutY="75.0" text="Name" />
            <Label layoutX="28.0" layoutY="146.0" prefHeight="25.0" prefWidth="32.0" text="Email" />
         </children>
    </AnchorPane>
  </children>
</VBox>
